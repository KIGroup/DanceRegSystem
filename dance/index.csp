    <title ng-bind="'Система регистрации'|localize"></title>
    <link rel="shortcut icon" href="img/icon.png" type="image/x-icon">
    <meta charset="utf-8">
    <meta name="keywords" content="Регистрация участников WDSF, СТСР."/>
